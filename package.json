{
  "name": "twitch-shared-community-graph-api",
  "version": "0.0.1",
  "description": "Small project to show a graph of shared communities. #Backend.",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "nodemon -L ./src/index.js",
    "lint": "eslint .",
    "test": "jest --collectCoverage  --testPathPattern=/src/test/unit --forceExit --passWithNoTests"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-throw-expressions"
    ]
  },
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>",
      "<rootDir>/src/test/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EsteveSegura/twitch-shared-community-graph-api.git"
  },
  "keywords": [
    "twitch",
    "graph",
    "stats"
  ],
  "author": "Esteve \"GiR\" Segura",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EsteveSegura/twitch-shared-community-graph-api/issues"
  },
  "homepage": "https://github.com/EsteveSegura/twitch-shared-community-graph-api#readme",
  "dependencies": {
    "awilix": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jest": "^27.1.0",
    "nodemon": "^2.0.12",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^27.1.1",
    "node-mocks-http": "^1.9.0",
    "nodemon": "^2.0.4",
    "supertest": "^5.0.0"
  }
}
